{
  "name": "ScheddVM",
  "description": "Schedule VM",
  "version": "0.0.0",
  "private": true,
  "main": "src/index.js",
  "jsnext:main": "src/index.js",
  "module": "src/index.js",
  "license": "MIT",
  "repository": "danigb/ScheddVM",
  "files": [
    "src",
    "dist"
  ],
  "author": "",
  "keywords": [],
  "scripts": {
    "test": "jest --coverage --no-cache",
    "build": "rollup -cm -n ScheddVM -f iife -i src/index.js -o dist/ScheddVM.js && cp dist/*.js docs/",
    "prepublish": "npm run build",
    "release": "npm run build && npm test && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags && npm publish",
    "docs": "docco src/* src/**/* -o docs/literate"
  },
  "babel": {
    "presets": "es2015"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^3.0.0",
    "hyperapp": "^0.7.1",
    "hyperx": "^2.3.0",
    "jest": "^19.0.2",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-uglify": "^1.0.1"
  },
  "standard": {
    "ignore": [
      "dist/*"
    ]
  },
  "dependencies": {
    "gibberish-dsp": "^2.5.0"
  }
}
